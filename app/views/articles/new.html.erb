<h1>New Article</h1>
<p>Find me in app/views/articles/new.html.erb</p>
<p>
	<%= form_for :article do |f| %>
	  <p>
	    <%= f.label :title %><br>
	    <%= f.text_field :title %>
	  </p>
 
	  <p>
	    <%= f.label :text %><br>
	    <%= f.text_area :text %>
	  </p>
 
	  <p>
	    <%= f.submit %>
	  </p>
	<% end %>
</p>
